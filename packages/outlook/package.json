{
  "name": "@speak-strong/outlook",
  "version": "2.3.0",
  "description": "Outlook add-in for Speak Strong - transform weak language in emails",
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/start-dev.ts",
    "dev:vite": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "certs": "office-addin-dev-certs install",
    "validate": "office-addin-manifest validate manifest.json",
    "setup:win": "bun run scripts/create-dev-manifest.ts",
    "deploy:win": "bun run build && bun run scripts/deploy-windows.ts"
  },
  "dependencies": {
    "@speak-strong/core": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/office-js": "^1.0.430",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "office-addin-debugging": "^5.1.6",
    "office-addin-dev-certs": "^2.0.6",
    "typescript": "^5.0.0",
    "vite": "^6.3.5"
  }
}
